          "Simple Calculator"
        ],
        "operationId": "Calculate",
        "deprecated": false,
        "produces": [
          "text/plain",
          "application/json"
        ],
        "parameters": [
          {
            "name": "operation",
            "in": "path",
            "required": true,
            "enum": [
              "SUM",
              "SUBTRACT",
              "MULTIPLY",
              "DIVIDE"
            ],
            "type": "string",
            "description": "The operator to apply on the variables"
          },
          {
            "name": "x",
            "in": "query",
            "required": true,
            "type": "number",
            "format": "double",
            "description": "The LHS value"
          },
          {
            "name": "y",
            "in": "query",
            "required": true,
            "type": "number",
            "format": "double",
            "description": "The RHS value"
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "type": "number",
              "format": "double"
            },
            "headers": {}
          },
          "412": {
            "description": "Could not compute the requested calculation",
            "schema": {
              "$ref": "#/definitions/CouldNotCompute"
            },
            "headers": {}
          }
        },
        "security": []
      }
    },
